#include<stdio.h>
int main()
{float a;
printf("enter your marks:");
scanf("%f",&a);
if(a<100&&a>85)
{
    printf("your grade is A");
}
else if(a<84&&a>70)
{
    printf("your grade is B");

}

else if(a<69&&a>55)
{
    printf("your grade is C");

}
else if (a<54&&a>40)
{
    printf("your grade is D");

}else {printf("your grade is F");};
return(0);
}
