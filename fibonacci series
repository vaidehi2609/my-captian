package test;

/**
 *
 * @author Admin
 */
public class Test {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int a=0,b=1,i,c;
        System.out.println("fibonacci series:");
        System.out.print(a+" "+b);
        for(i=1;i<10;i++){
            c=a+b;
            System.out.print(" "+c);
            a=b;
            b=c;
        }
        System.out.println();
    }
    
}
